{
  "name": "loxodonta",
  "version": "1.0.0",
  "description": "An implementation of the Mastodon/GNU Social protocols",
  "keywords": [
    "mastodon",
    "gnu social"
  ],
  "main": "index.js",
  "scripts": {
    "test": "tap test/*",
    "start": "node server.js"
  },
  "author": "Aria Stewart <aredridel@dinhe.net>",
  "license": "ISC",
  "devDependencies": {
    "tap": "^10.3.1"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "es6-promisify-all": "^0.1.0",
    "express": "^4.15.2",
    "flakeid": "^0.1.0",
    "form-data": "^2.1.4",
    "if-p": "^1.0.1",
    "internet-timestamp": "0.0.1",
    "level-promise": "^2.1.1",
    "level-sublevel": "^6.6.1",
    "leveldown": "^1.7.0-0",
    "levelgraph": "^2.0.1",
    "levelup": "^1.3.5",
    "ltx": "^2.7.1",
    "magic-signatures": "^1.0.4",
    "make-fetch-happen": "^2.2.4",
    "map-p": "^1.0.1",
    "promise": "^7.1.1",
    "replify": "^1.2.0",
    "stream-collector-p": "^1.0.1",
    "xml-schema": "^2.2.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6
    },
    "rules": {
      "valid-typeof": "error",
      "no-unused-vars": "error",
      "no-undef": "error"
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aredridel/loxodonta.git"
  },
  "bugs": {
    "url": "https://github.com/aredridel/loxodonta/issues"
  },
  "homepage": "https://github.com/aredridel/loxodonta#readme"
}
