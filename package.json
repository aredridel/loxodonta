{
  "name": "loxodonta",
  "version": "1.0.0",
  "description": "An implementation of the Mastodon/GNU Social protocols",
  "keywords": [
    "mastodon",
    "gnu social"
  ],
  "main": "index.js",
  "scripts": {
    "test": "tap test/*",
    "start": "node server.js"
  },
  "author": "Aria Stewart <aredridel@dinhe.net>",
  "license": "ISC",
  "devDependencies": {
    "tap": "^10.3.1"
  },
  "dependencies": {
    "express": "^4.15.2",
    "leveldown": "^1.7.0-0",
    "levelgraph": "^2.0.1",
    "levelup": "^1.3.5",
    "magic-signatures": "^1.0.0-2",
    "make-fetch-happen": "^2.2.1",
    "xml-schema": "^2.2.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6
    },
    "rules": {
      "valid-typeof": "error",
      "no-undef": "error"
    },
    "env": {
      "browser": true,
      "node": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aredridel/loxodonta.git"
  },
  "bugs": {
    "url": "https://github.com/aredridel/loxodonta/issues"
  },
  "homepage": "https://github.com/aredridel/loxodonta#readme"
}
